<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="ToolbarItem">
        <Properties>
          <Property
            Name="BoxShadowVShadow">0px</Property>
          <Property
            Name="BorderTop">solid 0px #000000</Property>
          <Property
            Name="BorderRight">solid 0px #000000</Property>
          <Property
            Name="BorderBottom">solid 0px #000000</Property>
          <Property
            Name="BorderLeft">solid 0px #000000</Property>
          <Property
            Name="BorderTopLeftRadius">0px</Property>
          <Property
            Name="BorderTopRightRadius">0px</Property>
          <Property
            Name="BorderBottomRightRadius">0px</Property>
          <Property
            Name="BorderBottomLeftRadius">0px</Property>
          <Property
            Name="BackgroundImage">url('right.png')</Property>
          <Property
            Name="BackgroundGradient">vertical transparent transparent</Property>
          <Property
            Name="BackgroundColor">transparent</Property>
          <Property
            Name="Width">30px</Property>
          <Property
            Name="Height">30px</Property>
        </Properties>
        <Classes>
          <StyleClass
            Name="NextTopic">
            <Properties>
              <Property
                Name="BackgroundColor">#ffffff</Property>
              <Property
                Name="BackgroundImage">url('ArrowRightSmall.png')</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
      <Style
        Name="Toolbar">
        <Properties>
          <Property
            Name="MarginTop">0px</Property>
          <Property
            Name="MarginBottom">0px</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="ArrowRightSmall.png"></Resource>
    </Resources>
  </Stylesheet>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="NextTopic" />
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="NextTopic">
                <Properties>
                  <Property
                    Name="Tooltip">Display next topic</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>